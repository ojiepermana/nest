{
  "name": "@ojiepermana/nest-generator",
  "version": "3.0.1",
  "description": "NestJS Code Generator Library - Generate CRUD modules from database metadata",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./audit": {
      "types": "./dist/audit/index.d.ts",
      "import": "./dist/audit/index.js",
      "require": "./dist/audit/index.js"
    },
    "./rbac": {
      "types": "./dist/rbac/index.d.ts",
      "import": "./dist/rbac/index.js",
      "require": "./dist/rbac/index.js"
    }
  },
  "bin": {
    "nest-generator": "dist/cli/index.js"
  },
  "author": "Ojie Permana",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ojiepermana/nest.git",
    "directory": "libs/generator"
  },
  "homepage": "https://github.com/ojiepermana/nest/tree/main/libs/generator#readme",
  "bugs": {
    "url": "https://github.com/ojiepermana/nest/issues"
  },
  "keywords": [
    "nestjs",
    "generator",
    "code-generator",
    "crud-generator",
    "database-metadata",
    "postgresql",
    "mysql",
    "typescript"
  ],
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=11.0.0"
  },
  "scripts": {
    "preinstall": "node -e \"if (process.env.npm_config_save_prod === 'true') { console.error('\\n⚠️  @ojiepermana/nest-generator should be installed as a dev dependency.\\n   Use: npm install --save-dev @ojiepermana/nest-generator\\n'); process.exit(1); }\"",
    "postinstall": "node -e \"console.log('\\n✅ @ojiepermana/nest-generator installed successfully!\\n   Run: npx nest-generator --help\\n')\""
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.0.0",
    "pg": "^8.13.0",
    "mysql2": "^3.11.0",
    "commander": "^12.1.0",
    "inquirer": "^8.2.0",
    "chalk": "^4.1.0",
    "ora": "^5.4.0",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "INSTALLATION.md",
    "DATABASE_COMPATIBILITY.md"
  ]
}
