{
  "name": "nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=11.0.0"
  },
  "scripts": {
    "build": "nest build",
    "build:generator": "nest build generator && npm run postbuild:generator",
    "build:nest": "nest build nest && npm run postbuild:nest",
    "postbuild:generator": "mkdir -p libs/generator/dist && cp -r dist/libs/generator/* libs/generator/dist/",
    "postbuild:nest": "mkdir -p libs/nest/dist && cp -r dist/libs/nest/* libs/nest/dist/",
    "build:all-libs": "npm run build:generator && npm run build:nest",
    "clean:dist": "rm -rf dist libs/generator/dist libs/nest/dist",
    "prebuild:libs": "npm run clean:dist && npm run build:all-libs",
    "publish:generator": "cd libs/generator && npm publish",
    "publish:nest": "cd libs/nest && npm publish",
    "publish:all-libs": "npm run prebuild:libs && npm run publish:generator && npm run publish:nest",
    "format": "prettier --write \"**/*.{ts,js,json,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\" --ignore-path .gitignore",
    "lint:md": "markdownlint \"**/*.md\" --ignore node_modules --ignore dist",
    "lint:md:fix": "markdownlint \"**/*.md\" --ignore node_modules --ignore dist --fix",
    "generator:init": "nest-generator init",
    "generator:generate": "nest-generator generate",
    "generator:list": "nest-generator list",
    "generator:link": "cd libs/generator && npm link",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/nest/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nest/test/jest-e2e.json",
    "test:api": "./test-api.sh",
    "test:api:http": "httpyac users-scenario.http --all",
    "test:api:verbose": "httpyac users-scenario.http --all --verbose",
    "test:api:junit": "httpyac users-scenario.http --all --junit --output test-results.xml",
    "test:load": "k6 run k6-test.js",
    "test:load:vus": "k6 run --vus 10 --duration 30s k6-test.js",
    "test:load:smoke": "k6 run --vus 1 --duration 10s k6-test.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/microservices": "^11.1.9",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "handlebars": "^4.7.8",
    "inquirer": "^8.2.6",
    "mysql2": "^3.11.5",
    "ora": "^5.4.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@types/express": "^5.0.5",
    "@types/handlebars": "^4.0.40",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "cache-manager": "^7.2.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "httpyac": "^6.16.7",
    "jest": "^30.2.0",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@ojiepermana/nest-generator(|/.*)$": "<rootDir>/libs/generator/src/$1",
      "^@ojiepermana/nest(|/.*)$": "<rootDir>/libs/nest/src/$1"
    }
  }
}
