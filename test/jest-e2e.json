{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@app/generator(|/.*)$": "<rootDir>/../libs/generator/src/$1",
    "^@app/nest(|/.*)$": "<rootDir>/../libs/nest/src/$1"
  }
}
